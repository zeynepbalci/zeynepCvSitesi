{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/deneyimlerim",
      "destination": "/deneyimlerim.html"
    },
    {
      "source": "/yeteneklerim",
      "destination": "/yeteneklerim.html"
    },
    {
      "source": "/iletisim",
      "destination": "/iletisim.html"
    },
    {
      "source": "/projelerim",
      "destination": "/projelerim.html"
    },
    {
      "source": "/planet-transition",
      "destination": "/planet-transition.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
